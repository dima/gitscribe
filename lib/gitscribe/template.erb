---
layout: post
title: <%= options[:title] %>
---

h1. {{ page.title }}

p(meta). <%= Date.today.strftime("%d %b %Y") %> - <%= options[:location] %>

<% chapters.each do |chapter| %>
h2. <%= chapter.title %>

<% chapter.sections.reverse.each do |section| %>
h3. Section

<%= section.message %>

<%= section.diff %>
<% end %>
<% end %>